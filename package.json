{
  "name": "slate-kit",
  "description": "A collection of slate's plugins and utils",
  "version": "0.2.18",
  "repository": "git://github.com/Vericus/slate-kit.git",
  "author": "Cadmus",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*",
    "tools/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap && yarn",
    "bootstrap:build": "yarn bootstrap && yarn build",
    "build-storybook": "build-storybook",
    "build": "yarn run build:packages && lerna run build --parallel",
    "build:clean": "lerna run clean:build && yarn run build",
    "build:packages": "rollup --config ./support/rollup/config.js",
    "clean": "lerna clean --yes && lerna run clean && rm -rf ./node_modules",
    "gh-pages": "NODE_ENV=gh-pages babel-node support/gh-pages/deploy.js",
    "lint": "tslint --project .",
    "new-module": "support/script/new-module",
    "prettier": "prettier --write '**/*.{js,jsx,md,json,css}'",
    "release:build": "lerna run clean:build && NODE_OPTIONS=--max-old-space-size=120000 NODE_ENV=production yarn build && NODE_OPTIONS=--max-old-space-size=120000 NODE_ENV=production yarn build:packages",
    "release:gh-pages": "yarn build-storybook && yarn gh-pages",
    "release": "yarn release:build && lerna publish",
    "storybook": "start-storybook -p 6006",
    "test": "jest --no-cache --logHeapUsage --colors",
    "updated": "lerna updated || exit 0",
    "watch": "npm-run-all --parallel --print-label watch:test watch:build watch:build-packages storybook",
    "watch:test": "yarn run test --watch",
    "watch:build-packages": "NODE_OPTIONS=--max-old-space-size=120000 yarn run build:packages --watch",
    "watch:build": "lerna run --parallel dev",
    "watch:dev": "npm-run-all --parallel --print-label watch:build watch:build-packages"
  },
  "husky": {
    "hooks": {
      "precommit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.0.2",
    "@storybook/addon-events": "^4.0.2",
    "@storybook/addon-info": "^4.0.2",
    "@storybook/addon-knobs": "^4.0.2",
    "@storybook/addon-links": "^4.0.2",
    "@storybook/addon-options": "^4.0.2",
    "@storybook/addon-storysource": "^4.0.2",
    "@storybook/addons": "^4.0.2",
    "@storybook/react": "^4.0.2",
    "@types/classnames": "^2.2.6",
    "@types/is-hotkey": "^0.1.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "@types/prop-types": "^15.5.5",
    "@types/react": "^16.7.3",
    "@types/react-dom": "^16.0.9",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "chalk": "^2.4.1",
    "child-process-promise": "^2.2.1",
    "chokidar": "^2.0.3",
    "compare-versions": "^3.4.0",
    "cross-env": "^5.2.0",
    "dtslint": "github:Microsoft/dtslint#production",
    "fs-extra": "^7.0.1",
    "get-monorepo-packages": "^1.1.0",
    "gh-pages": "^2.0.1",
    "husky": "^1.1.3",
    "is-hotkey": "*",
    "jest": "^23.6.0",
    "lerna": "^3.4.3",
    "lint-staged": "^8.0.4",
    "lodash": "^4.17.10",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.14.3",
    "prop-types": "^15.6.1",
    "quill-icons": "^1.5.0",
    "react-color": "^2.14.1",
    "react-popover": ">=0.5.4",
    "react-test-renderer": "^16.6.3",
    "recompose": "^0.30.0",
    "rmfr": "^2.0.0",
    "rollup": "^1.1.2",
    "rollup-plugin-analyzer": "^3.0.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript2": "^0.19.2",
    "simulate-event": "^1.4.0",
    "slate": ">=0.44.0 <0.45.0",
    "slate-base64-serializer": "*",
    "slate-dev-environment": "*",
    "slate-hotkeys": "*",
    "slate-html-serializer": "*",
    "slate-hyperscript": "*",
    "slate-plain-serializer": "*",
    "slate-prop-types": "*",
    "slate-react": ">=0.20.0 <0.22.0",
    "slate-simulator": "*",
    "storybook-readme": "^4.0.2",
    "styled-components": "^4.1.3",
    "tinymce-word-paste-filter": "^0.6.1",
    "to-camel-case": "^1.0.0",
    "ts-jest": "^23.10.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.6",
    "yo": "^2.0.1"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "immutable": ">=3.8.2",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "resolutions": {
    "memoize-one": "^5.0.0",
    "**/memoize-one": "^5.0.0"
  }
}
