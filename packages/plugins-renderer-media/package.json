{
  "version": "0.1.23",
  "name": "@vericus/slate-kit-media-renderer",
  "description": "TODO: fill in this description later",
  "homepage": "https://github.com/Vericus/slate-kit/",
  "repository": "git://github.com/Vericus/slate-kit.git",
  "author": "Cadmus",
  "slate-kit": {
    "type": "module"
  },
  "files": [
    "lib/"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/slate-kit-media-renderer.cjs.js",
  "types": "lib/index.d.ts",
  "module": "lib/slate-kit-media-renderer.es.js",
  "umd": "lib/slate-kit-media-renderer.umd.js",
  "umdMin": "lib/slate-kit-media-renderer.umd.min.js",
  "scripts": {
    "clean": "yarn run clean:module && yarn run clean:build",
    "clean:module": "rm -rf ./node_modules",
    "clean:build": "rm -rf ./lib ./.tsbuildinfo",
    "type-check": "tsc --noEmit",
    "type-check:watch": "TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling npm run type-check -- --watch",
    "build": "yarn clean && yarn build:types && yarn build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "cross-env BABEL_ENV=build babel src --out-dir lib --source-maps --extensions .ts,.tsx --config-file ../../babel.config.js --no-comments",
    "build:watch": "npm-run-all -p build:js-watch build:types-watch",
    "build:js-watch": "yarn run build:js --watch",
    "build:types-watch": "TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling npm run build:types -- --watch",
    "postbuild": "rollup -c ./rollup.config.ts",
    "postbuild:watch": "npm run postbuild -- --watch",
    "watch": "npx npm-run-all -l -r -n -p *:watch"
  },
  "dependencies": {
    "@vericus/slate-kit-utils-placeholders": "^0.1.18",
    "@vericus/slate-kit-utils-register-helpers": "^0.1.12",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@vericus/slate-kit-typescript-typings": "^1.1.13",
    "@vericus/slate-kit-utils-hyperscript": "^0.2.14",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0",
    "slate": ">=0.44.0 <0.46.0",
    "slate-prop-types": ">=0.5.7"
  },
  "peerDependencies": {
    "@vericus/slate-kit-utils-placeholders": "^0.1.0",
    "@vericus/slate-kit-utils-register-helpers": "^0.1.0",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0",
    "slate": ">=0.44.0 <0.46.0",
    "slate-prop-types": ">=0.5.7"
  },
  "keywords": [
    "Vericus",
    "slate",
    "slate-kit"
  ]
}
