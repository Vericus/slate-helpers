{
  "name": "@vericus/slate-kit-bind-hotkey",
  "version": "0.0.7",
  "description": "a slate helper plugin to associate hotkey(s) to slate change",
  "repository": "git://github.com/Vericus/slate-kit.git",
  "author": "Cadmus",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/slate-kit-bind-hotkey.js",
  "module": "lib/slate-kit-bind-hotkey.es.js",
  "umd": "dist/slate-kit-bind-hotkey.js",
  "umdMin": "dist/slate-kit-bind-hotkey.min.js",
  "files": [
    "dist/",
    "lib/",
    "module/"
  ],
  "slate-kit": {
    "type": "module"
  },
  "scripts": {
    "clean": "yarn run clean:module && yarn run clean:build",
    "clean:module": "rm -rf ./node_modules",
    "clean:build": "rm -rf ./dist ./lib ./module",
    "build": "BABEL_ENV=build babel ./src --out-dir ./module",
    "dev": "yarn run build --watch"
  },
  "umdGlobals": {
    "immutable": "Immutable",
    "slate": "Slate",
    "is-hotkey": "IsHotkey"
  },
  "dependencies": {
    "debug": ">=3.1.0",
    "immutable": "^3.8.2"
  },
  "devDependencies": {
    "expect": "^23.1.0",
    "flow-bin": "0.74.0",
    "is-hotkey": ">=0.1.2",
    "slate": ">=0.33.0",
    "slate-simulator": "^0.4.33",
    "to-camel-case": "^1.0.0"
  },
  "peerDependencies": {
    "is-hotkey": ">=0.1.2",
    "slate": ">=0.32.0"
  },
  "keywords": [
    "Vericus",
    "slate",
    "slate-kit",
    "slate-plugin",
    "hotkeys"
  ]
}
