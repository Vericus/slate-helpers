{
  "version": "0.0.16",
  "name": "@vericus/slate-kit-basic-typhography",
  "description": "plugin that provides basic typography functionality to slate",
  "homepage": "https://github.com/Vericus/slate-kit/",
  "repository": "git://github.com/Vericus/slate-kit.git",
  "author": "Cadmus",
  "slate-kit": {
    "type": "module"
  },
  "files": [
    "dist/",
    "lib/",
    "module/"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/slate-kit-basic-typhography.js",
  "types": "lib/index.d.ts",
  "module": "lib/slate-kit-basic-typhography.es.js",
  "umd": "dist/slate-kit-basic-typhography.js",
  "umdMin": "dist/slate-kit-basic-typhography.min.js",
  "scripts": {
    "clean": "yarn run clean:module && yarn run clean:build",
    "clean:module": "rm -rf ./node_modules",
    "clean:build": "rm -rf ./dist ./lib ./module",
    "build": "tsc -b .",
    "dev": "tsc -w ."
  },
  "umdGlobals": {
    "immutable": "Immutable",
    "slate": "Slate",
    "is-plain-object": "IsPlainObject",
    "@vericus/slate-kit-basic-typhography-renderer": "TypographyRenderer",
    "@vericus/slate-kit-plugins-utils": "SlateKitPluginsutils"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "immutable": "^3.8.2",
    "is-plain-object": "^2.0.4",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "devDependencies": {
    "expect": "^23.1.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "slate": ">=0.32.0",
    "slate-prop-types": ">=0.4.0",
    "slate-simulator": "^0.4.33",
    "to-camel-case": "^1.0.0"
  },
  "peerDependencies": {
    "@vericus/slate-kit-basic-typography-renderer": "^0.0.2",
    "@vericus/slate-kit-plugins-utils": "^0.0.9",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0",
    "slate": ">=0.32.0",
    "slate-prop-types": ">=0.4.0"
  },
  "keywords": [
    "Vericus",
    "slate",
    "slate-kit",
    "slate-plugin",
    "editor",
    "typography"
  ]
}
