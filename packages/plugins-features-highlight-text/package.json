{
  "version": "0.0.15",
  "name": "@vericus/slate-kit-highlight-text",
  "description": "plugin that provide a way to color/highlight a group of text on slate",
  "homepage": "https://github.com/Vericus/slate-kit/",
  "repository": "git://github.com/Vericus/slate-kit.git",
  "author": "Cadmus",
  "files": [
    "dist/",
    "lib/",
    "module/"
  ],
  "slate-kit": {
    "type": "module"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/slate-kit-highlight-text.js",
  "module": "lib/slate-kit-highlight-text.es.js",
  "umd": "dist/slate-kit-highlight-text.js",
  "umdMin": "dist/slate-kit-highlight-text.min.js",
  "scripts": {
    "clean": "yarn run clean:module && yarn run clean:build",
    "clean:module": "rm -rf ./node_modules",
    "clean:build": "rm -rf ./dist ./lib ./module",
    "build": "BABEL_ENV=build babel ./src --out-dir ./module",
    "dev": "yarn run build --watch"
  },
  "umdGlobals": {
    "immutable": "Immutable",
    "slate": "Slate",
    "is-plain-object": "IsPlainObject",
    "@vericus/slate-kit-highlight-text-renderer": "HighlightTextRenderer",
    "@vericus/slate-kit-plugins-utils": "SlateKitPluginsutils"
  },
  "dependencies": {
    "immutable": "^3.8.2",
    "is-plain-object": "^2.0.4",
    "tinycolor2": "^1.4.1"
  },
  "devDependencies": {
    "@vericus/slate-kit-utils-hyperscript": "^0.0.3",
    "expect": "^23.1.0",
    "flow-bin": "0.74.0",
    "slate": ">=0.33.0",
    "to-camel-case": "^1.0.0"
  },
  "peerDependencies": {
    "@vericus/slate-kit-highlight-text-renderer": "^0.0.2",
    "@vericus/slate-kit-plugins-utils": "^0.0.9",
    "slate": ">=0.32.0"
  },
  "keywords": [
    "Vericus",
    "slate",
    "slate-kit",
    "slate-plugin",
    "editor",
    "color"
  ]
}
